<turbo-stream action="replace" target="work_session_<%= @work_session.id %>">
  <template>
    <tr id="work_session_<%= @work_session.id %>">
      <td><%= @work_session.id %></td>
      <td><%= @work_session.start_time.in_time_zone('Asia/Tokyo').strftime('%Y-%m-%d %H:%M:%S') %></td>
      <td><%= @work_session.end_time.in_time_zone('Asia/Tokyo').strftime('%Y-%m-%d %H:%M:%S') %></td>
    </tr>
  </template>
</turbo-stream>
